{
    "manifest_version": 3,
    "name": "Codii",
    "version": "1.0",
    "description": "Codii- The Leetcode Grinder",
    "action": {
        "default_popup": "./popup.html"
    },
    "background":{
     "service_worker":"background.js"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://leetcode.com/problems/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}